Properties: {
    author: "NGSPipes Team"
    description: "Study case split and merge"
    version:  "1.0"
}

Repositories: [
    ToolRepository repo: {
        location: "E:\\Work\\NGSPipes\\ngspipes2\\main\\engine\\engine_core\\src\\test\\resources\\tools_support"
    }
]

Outputs: {
    output1: merge[output]
}

Steps: [
    Step split: {
        exec: repo[Split_Merge][split]
        execution_context: "DockerConfig"
        inputs: {
            path: "E:\\Desktop\\NGSPipesTeam\\Demo\\Inputs\\human.fa"
            type: "split"
            skipLines: 3185
            fileExtension: ".fa"
            filePrefix: "seq"
        }
    }
    Step makeblastdb: {
        exec: repo[Blast][makeblastdb]
        execution_context: "DockerConfig"
        inputs: {
            dbtype: "prot"
            out: "allrefs"
            title: "allrefs"
            in: "E:\\Desktop\\NGSPipesTeam\\Demo\\Inputs\\caseStudy1Minimal\\allrefs.fna.pro"
        }
    }
    Step blastx: {
        exec: repo[Blast][blastx]
        execution_context: "DockerConfig"
        inputs: {
            db: makeblastdb[outFileName]
            query: split[outputs]
            out: "blast.out"
        }
        spread: {
            inputs_to_spread: [query]
        }
    }
    Step merge: {
        exec: repo[Split_Merge][mergeFiles]
        execution_context: "DockerConfig"
        inputs: {
            files: blastx[outFile]
            type: "mergefiles"
            fileExtension: ".fa"
            filePrefix: "merged"
        }
    }

]